{
  "name": "nodejs-observatory",
  "version": "1.0.0",
  "license": "proprietary",
  "description": "A Nodejs application that provides live monitoring of your application",
  "main": "server.js",
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run build-ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "serve": "node dist/server.js",
    "start": "npm run serve",
    "clean": "rm -rf dist",
    "test": "jest --forceExit --coverage --verbose",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "ts-node --transpile-only server.ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "author": "",
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-openid-connect": "^2.17.1",
    "uuid": "^11.0.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/heapdump": "^0.3.4",
    "@types/node": "^20.12.7",
    "@types/node-schedule": "^2.1.7",
    "@types/nodemailer": "^6.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^9.15.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "@sendgrid/mail": "^8.1.4",
    "agenda": "^5.0.0",
    "axios": "^1.7.9",
    "bull": "^4.16.4",
    "bunyan": "^1.8.15",
    "commander": "^12.1.0",
    "heapdump": "^0.3.15",
    "ioredis": "^5.4.1",
    "knex": "^3.1.0",
    "log4js": "^6.9.1",
    "lru-cache": "^11.0.2",
    "mailgun.js": "^10.2.4",
    "mysql2": "^3.11.4",
    "node-cache": "^5.1.2",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.16",
    "pino": "^9.5.0",
    "pusher": "^5.2.0",
    "quick-lru": "^7.0.0",
    "redis": "^4.7.0",
    "winston": "^3.17.0"
  }
}
